{
  "name": "@h4iuiuc/eslint-plugin",
  "version": "0.0.1",
  "description": "An ESLint plugin intended for use by Hack4Impact UIUC",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json && prettier --write dist/**/*.{js,json,md}",
    "clean": "rimraf dist/",
    "format": "prettier --write \"./**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"./**/*.{ts,json,md}\"",
    "lint": "eslint src tests --ext .ts && markdownlint README.md docs",
    "test": "npm run clean && tsc -p tsconfig.json && mocha --timeout 10000 --recursive dist/tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hack4impact-uiuc/eslint-plugin.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "Hack4Impact UIUC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hack4impact-uiuc/eslint-plugin/issues"
  },
  "homepage": "https://github.com/hack4impact-uiuc/eslint-plugin#readme",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "markdownlint-cli": "^0.17.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
